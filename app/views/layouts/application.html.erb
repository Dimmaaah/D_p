<!DOCTYPE html>
<html>
<head>
  <title>Dimma</title>
   <link rel="stylesheet" href="https://cdn.rawgit.com/konpa/devicon/master/devicon.min.css">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">

</head>
<body>
 	<%= render "shared/navbar" %>
    	<%= yield %>

<div class="footer">
  <div class="container">
    <div class="row">
      <div class="col-md-12 col-sm-12">
        <ul class="social-buttons"></ul>
          <li class="social-button"></li><%= link_to root_path, class: "social_links2" do %>
          <%= content_tag(:i, "", class: "fa fa-home fa-3x") %>
          <% end %></li>
          <li class="social-button"></li><%= link_to "http://linkedin.com/in/daemien-ho", class: "social_links2" do %>
              <%= content_tag(:i, "", class: "fa fa-linkedin fa-3x") %>
              <% end %></li>
          <li class="social-button"></li><%= link_to home_contact_path, class: "social_links2" do %>
              <%= content_tag(:i, "", class: "fa fa-envelope fa-3x") %>
              <% end %></li>
          <li class="social-button"></li><%= link_to "https://github.com/Dimmaaah", class: "social_links2" do %>
                <%= content_tag(:i, "", class: "fa fa-github fa-3x") %>
                <% end %></li>
        </ul>
      </div>
    </div>
  </div>
</div>



   <script type="text/javascript">
   $(document).ready(function() {
  $('#section_about').hide(function() {
    $('.white, #readmore').mouseenter(function() {
      $('#section_about').show(3000)
    });
  });
});

  $(document).ready(function() {
    $('#div_icons').hide(function() {
      $('#tools').mouseenter(function() {
        $('#div_icons').slideDown("fast")
      });
    });
  });

$(document).ready(function() {
  $("#seemore").on("mouseenter", function() {
    $("html,body").animate({"scrollTop":$("#new").offset().top},1500);
    return false;
  });
});


$(function() {
  $("#readmore").on("mouseenter", function() {
    $("html,body").animate({"scrollTop":$("#about_me").offset().top},2500);
    return false;
  });
});
</script>
</body>
</html>
